    <settings defaultResolver="public"/>

    <resolvers>
        <url name="public">
            <ivy pattern="http://ivy.zutubi.com/[organisation]/[module]/ivy-[revision].xml"/>
            <artifact pattern="http://ivy.zutubi.com/[organisation]/[module]/[type]s/[artifact]-[revision].[type]"/>
        </url>
    </resolvers>
</ivysettings>
